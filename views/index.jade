doctype 5
html(lang="en")
  head
    title D3 Book Examples
    link(rel='stylesheet', href='//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css')
  body
    div(class='container')
      div(class='page-header')
        h1 D3 Book Examples
      each chapter in chapters
        h3= chapter.name
        ul
          each example in chapter.examples
            li
              a(href=example.link)= example.name
